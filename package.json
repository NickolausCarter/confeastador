  "bugs": {
    "url": "https://github.com/NickolausCarter/confeastador/issues"
  },
  "homepage": "https://github.com/NickolausCarter/confeastador#readme",
  "dependencies": {
    "create-react-app": "^4.0.3",
    "if-env": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^6.2.0"
  }
}
